pipeline {
agent any
options {
timestamps()
}
parameters
{
choice(name='profileName',choices=['Qaclick','Google'],description='hello')
}
tools {
       jdk 'Java 8'
       maven 'Maven 3.6.3'
     }
stages {
stage('Checkout') {
steps {
git credentialsId: 'Suman Git', url: 'https://github.com/Suman-arch/fourth.git'
}
}
stage('Maven') {
steps {
bat "mvn clean compile"
}
}
stage('test'){
steps {
bat "mvn test -P${params.profileName}"
}
}
stage('report'){
steps {
echo "report is generating"
}
post {
      success {
       echo "success process"
      }
      failure {
       echo "failure processs"
      }
    }
}


}
}




















